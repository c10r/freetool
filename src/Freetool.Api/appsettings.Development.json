{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=/app/data/freetool.db"
  },
  "OpenFGA": {
    "ApiUrl": "http://openfga:8090",
    "StoreId": "",
    "OrgAdminEmail": "chander@usemotion.com"
  },
  "Auth": {
    "IAP": {
      "ValidateJwt": true,
      "JwtAssertionHeader": "X-Goog-Iap-Jwt-Assertion",
      "JwtAudience": "",
      "JwtIssuer": "https://cloud.google.com/iap",
      "JwtCertsUrl": "https://www.gstatic.com/iap/verify/public_key-jwk",
      "EmailHeader": "X-Goog-Authenticated-User-Email",
      "NameHeader": "X-Goog-Authenticated-User-Name",
      "PictureHeader": "X-Goog-Iap-Attr-Picture",
      "GroupsHeader": "X-Goog-Iap-Attr-Groups",
      "GroupsDelimiter": ","
    },
    "GoogleDirectory": {
      "Enabled": false,
      "AdminUserEmail": "",
      "Scope": "https://www.googleapis.com/auth/admin.directory.user.readonly",
      "OrgUnitKeyPrefix": "ou",
      "IncludeOrgUnitHierarchy": true,
      "CustomAttributeKeyPrefix": "custom"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information"
    }
  }
}
